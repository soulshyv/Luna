@model Luna.ViewModels.EntityViewModel

<div class="row">
    <div class="col-md-12 text-center">
        <h4>@(ViewBag.Title) - Modifier</h4>
    </div>
</div>
<div class="row">
    <div class="col-md-6 offset-md-3">
        <form asp-action="Add" method="post">
            <input type="hidden" asp-for="Id" value="@Model.Id" />
            
            @await Html.PartialAsync("Entity/_Form", Model)
        </form>
    </div>
</div>